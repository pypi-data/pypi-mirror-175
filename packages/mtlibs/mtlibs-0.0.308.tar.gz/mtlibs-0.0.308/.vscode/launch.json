    "version": "0.2.0",
    "configurations": [        
        {
            "name": "mtxcms",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "0.0.0.0:8000"
            ],
            "django": true,
            "justMyCode": true,
            "env": {
                // "DJANGO_DEBUG":"False"
            }
        },
        {
            "name": "mtmitm",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/mtmitm/main.py",
            "args": [
                // "runserver",
                // "--settings=mtxcms.settings",
                // "0.0.0.0:8000"
            ],
            // "django": true,
            "justMyCode": true
        },
        {
            "name": "postdog",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "--settings=posthog.settings",
                "0.0.0.0:8000"
            ],
            "django": true,
            "justMyCode": true
        }
        
        ,{
            "name": "cmd mtx_init_cloud",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "mtx_init_cloud"
            ],
            "django": true,
            "justMyCode": true
        },{
            "name": "cmd mtx_task_moni",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "mtx_task_moni"
            ],
            "django": true,
            "justMyCode": true
        }
        ,{
            "name": "cmd mtproxy",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "mtproxy"
            ],
            "django": true,
            "justMyCode": true
        }
        ,{
            "name": "mtworker",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/mtworker/celery.py",
            "args": [
                "mtx_init_cloud"
            ],
            "django": false,
            "justMyCode": false
        },{
            "name": "mtworker-v2",
            "type": "python",
            "request": "launch",
            "module": "celery",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "-A", 
                "mtworker", 
                "worker", 
                "-l", 
                "debug", 
                // "-Q", 
                // "mtworker"
            ]
        },
        {
            "name": "mtx_cloud_worker",
            "type": "python",
            "request": "launch",
            "module": "celery",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "-A", 
                "mtx_cloud", 
                "worker", 
                "-l", 
                "debug",
                "-n",
                "mtx_cloud_worker_debug@%h"
                // "-Q", 
                // "mtworker"
            ]
        },


        {
            "name": "mtxcms_worker",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/mtxcms/celery.py",            
            "django": false,
            "justMyCode": false
        },
        

        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },

        {
            "name": "Python: Remote Attach",
            "type": "python",
            "request": "attach",
            "port": 5679, 
            "host": "127.0.0.1", 
            "pathMappings": [
                  {
                        "localRoot": "${workspaceFolder}",
                        "remoteRoot": "/home/code/workspace/mtxcms/"
                  }
            ]
      }
    ]
}