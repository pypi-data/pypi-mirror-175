{
    "manifest_version": "1",
    "task_list":
[
    {
        "name": "Create OME-ZARR structure",
        "input_type": "image",
        "output_type": "zarr",
        "default_args": {
            "num_levels": 2,
            "coarsening_xy": 2,
            "metadata_table": "mrf_mlf",
            "channel_parameters": null
        },
        "executable": "create_zarr_structure",
        "meta": {"executor": "cpu-low"}
    },
    {
        "name": "Yokogawa to Zarr",
        "input_type": "zarr",
        "output_type": "zarr",
        "default_args": {"parallelization_level": "well"},
        "executable": "yokogawa_to_zarr",
        "meta": {"executor": "cpu-low"}
    },
    {
        "name": "Replicate Zarr structure",
        "input_type": "zarr",
        "output_type": "zarr",
        "default_args": {
            "project_to_2D": true,
            "suffix": "mip"
        },
        "executable": "replicate_zarr_structure",
        "meta": {"executor": "cpu-low"}
    },
    {
        "name": "Maximum Intensity Projection",
        "input_type": "zarr",
        "output_type": "zarr",
        "default_args": {"parallelization_level": "well"},
        "executable": "maximum_intensity_projection",
        "meta": {"executor": "cpu-low"}
    },
    {
        "name": "Cellpose Segmentation",
        "input_type": "zarr",
        "output_type": "zarr",
        "default_args": {
            "labeling_channel": "A01_C01",
            "parallelization_level": "well"
        },
        "executable": "cellpose_segmentation",
        "meta": {"executor": "gpu"}
    },
    {
        "name": "Measurement",
        "input_type": "zarr",
        "output_type": "zarr",
        "default_args": {
            "labeling_channel": "A01_C01",
            "level": 0,
            "measurement_table_name": "nuclei",
            "parallelization_level": "well"
        },
        "executable": "measurement",
        "meta": {"executor": "cpu-low"}
    },
    {
        "name": "Illumination correction",
        "input_type": "zarr",
        "output_type": "zarr",
        "default_args": {
            "overwrite": false,
            "dict_corr": null,
            "background": 100,
            "parallelization_level": "well"
        },
        "executable": "illumination_correction",
        "meta": {"executor": "cpu-low"}
    }
]
}
